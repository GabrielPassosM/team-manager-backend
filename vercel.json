{
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "crons": [
    {
      "path": "/cron/test",
      "schedule": "30 23 * * *"
    }
  ]
}
